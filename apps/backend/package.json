{
  "name": "ai-job-hunter",
  "version": "1.0.0",
  "description": "AI-powered job hunting agent that searches, analyzes, and curates job postings",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "pnpm install && pnpm build && node dist/index.js",
    "dev": "nodemon --exec \"pnpm build && node dist/index.js\" --watch src --ext ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "install:deps": "pnpm install"
  },
  "keywords": [
    "job-hunting",
    "ai",
    "web-scraping",
    "automation",
    "email",
    "nodejs",
    "typescript"
  ],
  "author": "Sven Soldin",
  "license": "MIT",
  "dependencies": {
    "@mistralai/mistralai": "^1.10.0",
    "@supabase/supabase-js": "^2.58.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.0",
    "helmet": "^8.1.0",
    "puppeteer": "^22.0.0",
    "ts-node": "^10.9.2",
    "validator": "^13.15.20",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2",
    "@types/express": "^5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.0",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
